{
	"description": "Responsive Financial Times page footer",
	"origamiType": "module",
	"origamiCategory": "components",
	"origamiVersion": 1,
	"support": "https://github.com/Financial-Times/o-footer/issues",
	"supportStatus": "active",
	"browserFeatures": {
		"required": [
			"html5"
		],
		"optional": [
			"svg",
			"datauri"
		]
	},
	"ci": {
		"circle":"https://circleci.com/api/v1/project/Financial-Times/o-footer"
	},
	"demosDefaults": {
		"js": "demos/src/demo.js",
		"sass": "demos/src/demo.scss",
		"dependencies": "o-fonts"
	},
	"demos": [
		{
			"name": "footer",
			"template": "demos/src/footer.mustache",
			"data": "demos/src/footer.js",
			"description": ""
		},
		{
			"name": "footer-light",
			"template": "demos/src/footer.mustache",
			"data": "demos/src/footer-light.js",
			"description": ""
		},
		{
			"name": "simple-footer",
			"template": "demos/src/simple-footer.mustache",
			"description": "Simple footer for shorter stand-alone pages. The links in this demo must be included at the request of the FT's legal team."
		},
		{
			"name": "pa11y",
			"template": "demos/src/pa11y.mustache",
			"hidden": true,
			"description": ""
		}
	]
}
